<?xml version="1.0" encoding="utf-8"?>
<com.weygo.common.widget.JHNavigationBar xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="44dp"
    android:background="@color/WGAppBaseColor"
    android:fitsSystemWindows="true" >

    <ImageView
        android:id="@+id/titlebar_left"
        android:layout_width="21dp"
        android:layout_height="21dp"
        android:layout_marginTop="@dimen/x9"
        android:layout_marginLeft="@dimen/x14"
        android:src="@drawable/common_return"
        />

    <TextView
        android:id="@+id/titlebar_title"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="50dp"
        android:gravity="center"
        android:textSize="16sp"
        android:textColor="@color/navigationBar_title"/>

    <ImageView
        android:id="@+id/titlebar_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="15dp"
        android:layout_centerVertical="true" />

</com.weygo.common.widget.JHNavigationBar>