<?xml version="1.0" encoding="utf-8"?>
<com.weygo.weygophone.pages.classifyDetail.widget.WGClassifyPopView xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="@dimen/x170"
        android:layout_height="@dimen/x270"
        android:background="@drawable/classifydetail_pop_bg"
        android:layout_marginTop="64dp">

        <android.support.v7.widget.RecyclerView
            android:id="@+id/recyclerView"
            android:divider="#ff000000"
            android:dividerHeight="10dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="8dp"/>

    </LinearLayout>


</com.weygo.weygophone.pages.classifyDetail.widget.WGClassifyPopView>