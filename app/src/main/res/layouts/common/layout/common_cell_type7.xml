<?xml version="1.0" encoding="utf-8"?>
<com.weygo.weygophone.common.widget.WGCellStyle7View xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="@dimen/x41"
    android:background="#f8FAFA">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="@dimen/x21"
        android:layout_height="@dimen/x21"
        android:layout_marginLeft="@dimen/x13"
        android:layout_marginTop="@dimen/x10"
        android:src="@drawable/common_location"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toLeftOf="@+id/arrImageView"
        android:layout_toRightOf="@+id/imageView"
        android:layout_marginLeft="@dimen/x7"
        android:layout_marginRight="@dimen/x7"
        android:gravity="center_vertical"
        android:textSize="@dimen/ft12"
        />

</com.weygo.weygophone.common.widget.WGCellStyle7View>