<?xml version="1.0" encoding="utf-8"?>
<com.weygo.weygophone.common.widget.WGCellStyle5View xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/x41"
    android:background="#ffffff">

    <TextView
        android:id="@+id/titleView"
        android:layout_width="@dimen/x140"
        android:layout_height="match_parent"
        android:paddingLeft="@dimen/x13"
        android:gravity="center_vertical|left"
        android:textSize="@dimen/ft12"
        android:text="@string/PersonInfo_FirstName"
        android:textColor="@color/WGAppBaseTitleAAAColor"
        />
    
    <EditText
        android:id="@+id/titleValueET"
        android:layout_width="@dimen/x180"
        android:layout_height="match_parent"
        android:layout_marginRight="@dimen/x13"
        android:background="@null"
        android:gravity="center_vertical|right"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:textSize="@dimen/ft12"
        android:textColor="@color/WGAppBaseTitleAColor"
        android:layout_marginEnd="14dp" />

</com.weygo.weygophone.common.widget.WGCellStyle5View>