<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/item"
    android:layout_width="@dimen/x109"
    android:layout_height="@dimen/x55"
    android:background="@drawable/wgslider_topic_item_corner">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="@dimen/x26"
        android:layout_height="@dimen/x26"
        android:layout_marginLeft="@dimen/x11"
        android:layout_marginTop="@dimen/x15" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toRightOf="@+id/imageView"
        android:layout_marginLeft="@dimen/x7"
        android:layout_marginRight="@dimen/x7"
        android:gravity="center_vertical"
        android:textSize="@dimen/ft10"
        />

</RelativeLayout>