<?xml version="1.0" encoding="utf-8"?>
<com.weygo.weygophone.pages.coupon.widget.WGCouponListInputView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/x38"
    android:gravity="center">

    <EditText
        android:id="@+id/inputET"
        android:layout_width="@dimen/x224"
        android:layout_height="@dimen/x24"
        android:textColor="@color/WGAppBaseTitleAAAAColor"
        android:textSize="@dimen/ft13"
        android:gravity="center_horizontal"
        android:hint="@string/CouponList_Input_Holder"/>

    <View
        android:layout_width="@dimen/x10"
        android:layout_height="1dip"></View>

    <TextView
        android:id="@+id/activeTV"
        android:layout_width="@dimen/x75"
        android:layout_height="@dimen/x21"
        android:textColor="@color/WGAppBlueButtonColor"
        android:background="@drawable/address_use_bg"
        android:text="@string/CouponList_Active"
        android:gravity="center"
        android:textSize="@dimen/ft12"/>

</com.weygo.weygophone.pages.coupon.widget.WGCouponListInputView>