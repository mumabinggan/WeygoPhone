<?xml version="1.0" encoding="utf-8"?>
<com.weygo.weygophone.pages.personInfo.widget.WGPersonInfoHeadView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/x68"
    android:background="#ffffff">

    <TextView
        android:id="@+id/textView"
        android:layout_width="@dimen/x160"
        android:layout_height="match_parent"
        android:paddingLeft="@dimen/x13"
        android:gravity="center_vertical|left"
        android:textSize="@dimen/ft12"
        android:text="@string/PersonInfo_Image"
        android:textColor="@color/WGAppBaseTitleAAAColor"
        />
    
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="@dimen/x48"
        android:layout_height="@dimen/x48"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/x13"
        android:layout_marginTop="@dimen/x10"/>

</com.weygo.weygophone.pages.personInfo.widget.WGPersonInfoHeadView>