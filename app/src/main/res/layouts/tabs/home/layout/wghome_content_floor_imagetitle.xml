<?xml version="1.0" encoding="utf-8"?>
<com.weygo.weygophone.pages.tabs.home.widget.WGHomeContentFloorImageTitleView xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="@dimen/x116">

    <ImageView
        android:id="@id/imageView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <TextView
        android:id="@+id/nameTV"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x24"
        android:textSize="@dimen/ft19"
        android:textColor="#ffffff"
        android:layout_marginTop="@dimen/x22"
        android:textAlignment="center"
        android:gravity="center"/>

    <TextView
        android:id="@+id/breifDesTV"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x17"
        android:textSize="@dimen/ft12"
        android:layout_below="@+id/nameTV"
        android:textColor="#ffffff"
        android:gravity="center"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/moreTV"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x20"
        android:textSize="@dimen/ft12"
        android:layout_below="@+id/breifDesTV"
        android:layout_marginTop="@dimen/x10"
        android:textColor="#ffffff"
        android:gravity="center"
        android:background="@drawable/wghome_content_floor_imagetitle_more"
        android:textAlignment="center" />

</com.weygo.weygophone.pages.tabs.home.widget.WGHomeContentFloorImageTitleView>