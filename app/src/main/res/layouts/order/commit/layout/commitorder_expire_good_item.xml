<?xml version="1.0" encoding="utf-8"?>
<com.weygo.weygophone.pages.order.commit.widget.WGCommitOrderExpireItemView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" 
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="@dimen/x51"
        android:layout_height="@dimen/x51"
        android:layout_marginLeft="@dimen/x30"/>
    
    <TextView
        android:id="@+id/nameTextView"
        android:layout_width="@dimen/x140"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/imageView"
        android:textColor="#000000"
        android:layout_marginLeft="@dimen/x5"
        android:textSize="@dimen/x12"
        android:maxLines="2"/>

    <TextView
        android:id="@+id/expireTimeTextView"
        android:layout_width="@dimen/x140"
        android:layout_height="wrap_content"
        android:layout_below="@+id/nameTextView"
        android:layout_marginTop="@dimen/x5"
        android:layout_alignLeft="@+id/nameTextView"
        android:textColor="#ff0000"
        android:textSize="@dimen/x12"
        android:maxLines="1"/>
    
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/x60"></View>
    
</com.weygo.weygophone.pages.order.commit.widget.WGCommitOrderExpireItemView>