<?xml version="1.0" encoding="utf-8"?>
<com.weygo.weygophone.pages.order.commit.widget.WGCommitOrderOverWeightItemView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" 
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="@dimen/x60"
        android:layout_height="@dimen/x60"
        android:layout_marginLeft="@dimen/x10"/>
    
    <TextView
        android:id="@+id/nameTextView"
        android:layout_width="@dimen/x160"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/imageView"
        android:textColor="#000000"
        android:layout_marginLeft="@dimen/x5"
        android:textSize="@dimen/x12"
        android:maxLines="1"/>

    <TextView
        android:id="@+id/briefTextView"
        android:layout_width="@dimen/x160"
        android:layout_height="wrap_content"
        android:layout_below="@+id/nameTextView"
        android:layout_marginTop="@dimen/x5"
        android:layout_alignLeft="@+id/nameTextView"
        android:textColor="#000000"
        android:textSize="@dimen/x12"
        android:maxLines="1"/>

    <include
        android:id="@+id/addView"
        android:layout_width="@dimen/x80"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/briefTextView"
        android:layout_below="@+id/priceView"
        android:layout_marginTop="@dimen/x5"
        layout="@layout/common_addgood" />
    
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/x70"></View>
    
</com.weygo.weygophone.pages.order.commit.widget.WGCommitOrderOverWeightItemView>