<?xml version="1.0" encoding="utf-8"?>
<com.weygo.weygophone.pages.order.commit.widget.WGCommitOrderLookMoreTipsView xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/x13"
        android:gravity="center">

        <TextView
            android:id="@+id/tipsTV"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/ft12"
            android:textColor="#000000"
            android:layout_weight="1"/>

        <TextView
            android:id="@+id/moreTV"
            android:layout_width="@dimen/x50"
            android:layout_height="wrap_content"
            android:textSize="@dimen/ft12"
            android:textColor="@color/WGAppBaseColor"
            android:text="@string/CommitOrder_Look_More"/>

    </LinearLayout>


</com.weygo.weygophone.pages.order.commit.widget.WGCommitOrderLookMoreTipsView>