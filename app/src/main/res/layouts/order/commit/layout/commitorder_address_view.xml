<?xml version="1.0" encoding="utf-8"?>
<com.weygo.weygophone.pages.order.commit.widget.WGCommitOrderAddressView xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" 
    android:layout_width="match_parent"
    android:layout_height="@dimen/x82"
    android:background="#ffffff">
    
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x68"
        android:layout_gravity="center"
        android:src="@drawable/commitOrder_address_bg"
        android:layout_marginTop="@dimen/x7"/>

    <TextView
        android:id="@+id/nameTV"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x68"
        android:maxLines="2"
        android:textSize="@dimen/ft12"
        android:textColor="#000000"
        android:gravity="left|center_vertical"
        android:layout_marginTop="@dimen/x7"
        android:layout_marginLeft="@dimen/x48"
        android:layout_marginRight="@dimen/x20"/>

    <ImageView
        android:id="@+id/arrImageView"
        android:layout_width="@dimen/x6"
        android:layout_height="@dimen/x10"
        android:layout_alignParentRight="true"
        android:layout_marginRight= "@dimen/x13"
        android:layout_marginTop="@dimen/x36"
        android:src="@drawable/common_right_arr"/>

</com.weygo.weygophone.pages.order.commit.widget.WGCommitOrderAddressView>