<?xml version="1.0" encoding="utf-8"?>
<com.weygo.weygophone.pages.order.detail.widget.WGOrderDetailPayView xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/userTitleTV"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x34"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/x14"
        android:text="@string/OrderDetail_Pay_Info"
        android:background="#f8fafa"
        android:textColor="@color/WGAppBaseTitleAColor"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffffff">
        
        <View
            android:id="@+id/headerView"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x14"></View>

        <TextView
            android:id="@+id/pay1TV"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x17"
            android:gravity="center_vertical"
            android:layout_below="@+id/headerView"
            android:paddingLeft="@dimen/x14"
            android:visibility="gone"
            android:textColor="@color/WGAppBaseTitleAAColor"/>

        <TextView
            android:id="@+id/pay2TV"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x17"
            android:gravity="center_vertical"
            android:layout_below="@+id/pay1TV"
            android:paddingLeft="@dimen/x14"
            android:text="2323"
            android:visibility="gone"
            android:textColor="@color/WGAppBaseTitleAAColor"/>

        <TextView
            android:id="@+id/pay3TV"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x17"
            android:gravity="center_vertical"
            android:layout_below="@+id/pay2TV"
            android:paddingLeft="@dimen/x14"
            android:text="2323"
            android:visibility="gone"
            android:textColor="@color/WGAppBaseTitleAAColor"/>

        <TextView
            android:id="@+id/pay4TV"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x17"
            android:gravity="center_vertical"
            android:layout_below="@+id/pay3TV"
            android:layout_marginBottom="@dimen/x14"
            android:paddingLeft="@dimen/x14"
            android:text="2323"
            android:visibility="gone"
            android:textColor="@color/WGAppBaseTitleAAColor"/>

        <TextView
            android:id="@+id/pay5TV"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x17"
            android:gravity="center_vertical"
            android:layout_below="@+id/pay4TV"
            android:layout_marginBottom="@dimen/x14"
            android:paddingLeft="@dimen/x14"
            android:text="2323"
            android:visibility="gone"
            android:textColor="@color/WGAppBaseTitleAAColor"/>

        <TextView
            android:id="@+id/pay6TV"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x17"
            android:gravity="center_vertical"
            android:layout_below="@+id/pay5TV"
            android:layout_marginBottom="@dimen/x14"
            android:paddingLeft="@dimen/x14"
            android:text="2323"
            android:visibility="gone"
            android:textColor="@color/WGAppBaseTitleAAColor"/>

        <TextView
            android:id="@+id/pay7TV"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x17"
            android:gravity="center_vertical"
            android:layout_below="@+id/pay6TV"
            android:paddingLeft="@dimen/x14"
            android:text="2323"
            android:visibility="gone"
            android:textColor="@color/WGAppBaseTitleAAColor"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/x14"
            android:layout_below="@+id/pay7TV">

        </View>

    </RelativeLayout>

</com.weygo.weygophone.pages.order.detail.widget.WGOrderDetailPayView>